<div class="box-clientsRegistration">
  <div class="movap-titulo">
    <h4 align="left">CADASTRO DE GESTORES</h4>
    <hr>
  </div>

  <form action="/manager/signup" class="col-lg-10" method="post">
    <div class="form-group col-lg-8">
      <p class="movap-nomes" align="left">Código do Gestor</p>
      <input type="text" class="form-control" id="intentificação" name="manager[codManager]"placeholder="">
    </div>
    <div class="form-group col-lg-8">
      <p class="movap-nomes" align="left">CPF</p>
      <input type="number" class="form-control" id="intentificação" name="manager[cpf]"placeholder="">
    </div>

  <div class="form-row pl-3">
      <div class="form-group col-lg-6 col-9">
        <p class="movap-nomes" align="left" for="inputAddress">Endereço</p>
        <input type="text" class="form-control" id="inputAddress" placeholder="" name="manager[address][street]">
      </div>
      <div class="form-group col-lg-2 col-3 pr-4">
        <p class="movap-nomes" align="left">Número</p>
        <input type="number" class="form-control" name="manager[address][number]">
      </div>
    </div>
    <div class="form-row pl-3">
      <div class="form-group col-lg-4 col-6 pr-2">
        <p class="movap-nomes" align="left">Cidade</p>
        <input type="text" class="form-control" id="inputCity" name="manager[address][city]">
      </div>
      <div class="form-group col-lg-2 col-3 pr-2 pl-2">
        <p class="movap-nomes" align="left">Estado</p>
        <select id="inputEstado" class="form-control">
          <option selected>Escolher...</option>
          <option>AC</option>
          <option>AL</option>
          <option>AP</option>
          <option>AM</option>
          <option>BA</option>
          <option>CE</option>
          <option>DF</option>
          <option>ES</option>
          <option>GO</option>
          <option>MA</option>
          <option>MT</option>
          <option>MS</option>
          <option>MG</option>
          <option>PA</option>
          <option>PB</option>
          <option>PR</option>
          <option>PE</option>
          <option>PI</option>
          <option>RJ</option>
          <option>RN</option>
          <option>RS</option>
          <option>RO</option>
          <option>RR</option>
          <option>SC</option>
          <option>SP</option>
          <option>SE</option>
          <option>TO</option>
        </select>
      </div>
      <div class="form-group col-lg-2 col-3 pr-4">
        <p class="movap-nomes" align="left">CEP</p>
        <input type="number" class="form-control" id="inputCEP" name="manager[address][cep]">
      </div>
    </div>
    <div class="form-group col-lg-8">
      <p class="movap-nomes" align="left">E-mail</p>
      <input type="email" class="form-control" id="intentificação" name="manager[email]" placeholder="">
    </div>
    <div class="form-group col-lg-8">
      <p class="movap-nomes" align="left">Telefone</p>
      <input type="tel" class="form-control" id="intentificação" name="manager[phone]"placeholder="">
    </div>
    <div class="form-group col-lg-8">
      <p class="movap-nomes" align="left">Nome</p>
      <input type="text" class="form-control" id="intentificação" name="manager[name]"placeholder="">
    </div>
    <div class="form-group col-lg-8">
      <p class="movap-nomes" align="left">Status</p>
      <select class="form-control" id="exampleFormControlSelect1" name="manager[status]">
        <option>Ativo</option>
        <option>Outros</option>
      </select>
    </div>
    <div class="form-group justify-content-start col-md-8 d-flex movap">
      <input type="submit" value="CONFIRMAR" class="btn btn-primary" >
    </div>
  </form>
  <br><br><br><br>
</div>
